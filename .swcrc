{
    "sourceMaps": true,
    "minify": true,
    "env": {
        "coreJs": "3.22"
    },
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": false
        },
        "target": "es5",
        "externalHelpers": false,
        // Requires v1.2.50 or upper and requires target to be es2016 or upper.
        "keepClassNames": false,
        "transform": {
            "react": {
                "runtime": "automatic"
            }
        },
        "minify": {
            "compress": {
              "unused": true
            },
            "mangle": true
        }
    }
}
